<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Humana_Medicare_Advantage</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL(ClientName__c, "Humana"), Group_PlanSponsor__c &lt;&gt; "Humana Medicare Advantage")</errorConditionFormula>
    <errorDisplayField>Group_PlanSponsor__c</errorDisplayField>
    <errorMessage>Group/Plan Sponsor must be Humana Medicare Advantage</errorMessage>
</ValidationRule>
