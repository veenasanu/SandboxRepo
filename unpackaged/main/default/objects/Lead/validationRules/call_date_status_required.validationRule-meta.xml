<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>call_date_status_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
   NOT(ISBLANK( X1st_Call__c )),
   ISBLANK(TEXT( X1st_Call_Status__c ))
)</errorConditionFormula>
    <errorMessage>You must select a value for status</errorMessage>
</ValidationRule>
