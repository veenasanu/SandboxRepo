<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Premera_Validation</fullName>
    <active>false</active>
    <description>The group plan sponsor for Premera leads must be Amazon</description>
    <errorConditionFormula>And(
    ISPICKVAL(ClientName__c , "Premera"),
    Group_PlanSponsor__c &lt;&gt;  "Amazon",
    Group_PlanSponsor__c &lt;&gt; "Call Request"
)</errorConditionFormula>
    <errorDisplayField>Group_PlanSponsor__c</errorDisplayField>
    <errorMessage>The group plan sponsor for Premera leads must be Amazon</errorMessage>
</ValidationRule>
